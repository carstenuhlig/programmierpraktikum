CREATE TABLE sequences (id INTEGER UNSIGNED AUTO_INCREMENT PRIMARY KEY, sequence LONGTEXT, organism VARCHAR(50), dbsource VARCHAR(50), identifier VARCHAR(50));

CREATE TABLE orfs (id INTEGER UNSIGNED AUTO_INCREMENT PRIMARY KEY, fromseq INTEGER UNSIGNED, startpos INTEGER UNSIGNED, endpos INTEGER UNSIGNED);

CREATE TABLE keywords (id INTEGER UNSIGNED AUTO_INCREMENT PRIMARY KEY, fromseq INTEGER UNSIGNED, keyword VARCHAR(50))  

CREATE TABLE acession_nr (id INTEGER UNSIGNED AUTO_INCREMENT PRIMARY KEY, fromseq INTEGER UNSIGNED, acession VARCHAR(20))
